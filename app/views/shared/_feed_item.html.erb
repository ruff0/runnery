<li id="<%= feed_item.id %>">
  <span class="user">
    <%= link_to feed_item.user.address, feed_item.user %>
  </span><br>
  <span class="content"><%= feed_item.address %></span>
  <span class="timestamp"><br>
    <%= time_ago_in_words(feed_item.created_at) %> ago.
  </span>
  <% if current_user?(feed_item.user) %>

    <%= link_to "delete", feed_item, method: :delete,
                                     data: { confirm: "You sure?" },
                                     title: feed_item.user_id  %>

  
  <% end %>
</li><br>